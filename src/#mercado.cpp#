#include <iostream>
using namespace std;



int main(){
	Lista* l;
	l = inicializa();
	l = insere(l,23);
	l = insere(l,45);
	imprime(l);

	return 0;
}

Lista* inicializa (){
	return NULL;
}

Lista* insere (Lista* l, int i){
	Lista* novo = (Lista*) malloc(sizeof(Lista));
	novo->info = i;
	novo->prox = l;
	return novo;
}
void imprime (Lista* l){
	Lista* p;
		for(p=l;p != NULL; p = p->prox)
       		cout << "info = " << p->info << endl ;
}
